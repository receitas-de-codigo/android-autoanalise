                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".PerguntasActivity">
        </activity>
        <activity android:name=".ResultadoActivity">
        </activity>
    </application>

</manifest>